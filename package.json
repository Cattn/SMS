{
	"name": "sms",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"build:api": "cd api && npx webpack",
		"start:api": "cd api && node dist/server.cjs",
		"dev:api": "npm run build:api && npm run start:api",
		"Ldev:api": "npm run start:api",
		"start": "npm run build && cross-env BODY_SIZE_LIMIT=Infinity node server.js",
		"Lstart": "cross-env BODY_SIZE_LIMIT=Infinity node server.js",
		"startall": "concurrently \"npm run start\" \"npm run dev:api\"",
		"ncuStart": "concurrently \"npm run Lstart\" \"npm run dev:api\"",
		"nsuStart": "concurrently \"npm run start\" \"npm run Ldev:api\"",
		"nobStart": "concurrently \"npm run Lstart\" \"npm run Ldev:api\""
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"concurrently": "^9.1.2",
		"cross-env": "^7.0.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"ts-loader": "^9.5.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
		"vite-plugin-devtools-json": "^0.2.0",
		"webpack-cli": "^6.0.1"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "^5.2.12",
		"@types/express": "^5.0.3",
		"@types/express-fileupload": "^1.5.1",
		"busboy": "^1.6.0",
		"express": "^5.1.0",
		"express-fileupload": "^1.5.1",
		"iconify-icon": "^3.0.0",
		"m3-svelte": "^4.4.0",
		"retimer": "^4.0.0",
		"sqlite3": "^5.1.7",
		"webpack": "^5.99.9"
	}
}
